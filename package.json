{
  "name": "@josephmcg/config",
  "description": "A collection of config files",
  "packageManager": "pnpm@8.11.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/josephmcg/config.git"
  },
  "type": "module",
  "license": "MIT",
  "version": "0.4.1",
  "main": "dist/index.js",
