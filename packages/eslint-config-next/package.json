{
  "name": "@josephmcg/eslint-config-next",
  "version": "1.2.0",
  "description": "ESLint config for Next.js projects",
  "author": "josephmcg",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/josephmcg/config.git",
    "directory": "packages/eslint-config-next"
  },
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "\n============== Dev ===============": "",
    "dev": "tsdown --watch",
    "build": "tsdown",
    "release": "pnpm run build && changeset publish",
    "\n============== Lint ==============": "",
    "lint": "pnpm run '/^lint:.*/'",
    "lint:prettier": "prettier --cache --check src",
    "lint:tsc": "tsc --noEmit"
  },
  "dependencies": {
    "@josephmcg/eslint-config-react": "workspace:*"
  }
}
