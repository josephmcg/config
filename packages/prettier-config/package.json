{
  "name": "@josephmcg/prettier-config",
  "version": "2.0.0",
  "description": "Prettier config for my projects",
  "author": "josephmcg",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/josephmcg/config.git",
    "directory": "packages/prettier-config"
  },
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "\n============== Dev ===============": "",
    "dev": "tsup --watch",
    "build": "tsup",
    "release": "pnpm run build && changeset publish",
    "\n=========== Lint/Test ============": "",
    "lint": "pnpm run '/^lint:.*/'",
    "lint:eslint": "eslint --cache .",
    "lint:prettier": "prettier --cache --check src",
    "check-exports": "attw --profile esm-only --pack ."
  },
  "peerDependencies": {
    "prettier": "^3.0.0"
  }
}
