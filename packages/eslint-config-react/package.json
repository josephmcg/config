{
  "name": "@josephmcg/eslint-config-react",
  "version": "2.0.0",
  "description": "ESLint config for React projects",
  "author": "josephmcg",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/josephmcg/config.git",
    "directory": "packages/eslint-config-react"
  },
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "\n============== Dev ===============": "",
    "dev": "tsdown --watch",
    "build": "tsdown",
    "release": "pnpm run build && changeset publish",
    "\n============== Lint ==============": "",
    "lint": "pnpm run '/^lint:.*/'",
    "lint:prettier": "prettier --cache --check src",
    "lint:tsc": "tsc --noEmit"
  },
  "peerDependencies": {
    "@eslint/js": "^9.0.0",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.0.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^10.0.0",
    "eslint-plugin-jsx-a11y": "^6.0.0",
    "eslint-plugin-react": "^7.0.0",
    "eslint-plugin-react-hooks": "^7.0.0",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "eslint-plugin-unicorn": "^62.0.0",
    "eslint-plugin-unused-imports": "^4.0.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.0.0"
  }
}
